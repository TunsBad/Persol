@{
    ViewBag.Title = "Store Car";
}

<div class="col-md-12">
    <div class="col-md-5 col-md-offset-3">
        <form>
            <div class="form-group">
                <label for="carName">Name</label>
                <input type="text" class="form-control" id="carName" aria-describedby="emailHelp" placeholder="Enter car name">
            </div>
            <div class="form-group">
                <label for="carMake">Make</label>
                <input type="text" class="form-control" id="carMake" placeholder="Enter car make">
            </div>
            <div class="form-group">
                <label for="carYear">Year</label>
                <input class="form-control" id="carYear">
            </div>
            <div class="form-group">
                <label for="carModel">Model</label>
                <input type="text" class="form-control" id="carModel" placeholder="Enter model">
            </div>
            <button id="storeCar" type="button" class="btn btn-primary">&nbsp;Submit&nbsp;</button>
        </form>
    </div>
</div>

@section scripts {
    <script src="~/Scripts/app/storecar.js"></script>
}